{
	"name": "cyData",
	"version": "2.0",
	"description": "畅言数据插件",
	"manifest_version": 2,
    "browser_action": {
        "default_icon": "./images/icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "128": "./images/logo128.png",
        "16": "./images/logo16.png",
        "48": "./images/logo48.png"
    },
    "background": {
        "page": "./background.html"
    },
	"minimum_chrome_version": "18",
    "permissions": [
        "tabs",
        "storage",
        "http://*/*",
        "https://*/*",
        "ftp://*/*",
        "\u003Call_urls>"
    ]
}